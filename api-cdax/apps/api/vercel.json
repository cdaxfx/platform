{
    "version": 2,
    "name": "cdax-api",
    "builds": [
        {
            "src": "./src/main.ts",
            "use": "@vercel/node",
            "config": {
                "distdir": "./dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "./dist/main.js"
        }
    ]
}